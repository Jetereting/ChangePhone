<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#d80099fd"
    tools:context="us.eiyou.changephone.activity.BrandActivity">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="37dp"
        android:paddingBottom="@dimen/activity_vertical_margin">
    <EditText
        android:id="@+id/et_manufacturer"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="17dp"
        android:hint="你想修改成的生产厂家(比如 iphone)"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
    <EditText
        android:id="@+id/et_model"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="17dp"
        android:hint="你想修改成的手机型号(比如 7 plus)"
        android:layout_below="@+id/et_manufacturer"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
    <Button
        android:id="@+id/b_sure"
        android:layout_width="match_parent"
        android:layout_height="196px"
        android:background="@drawable/selector_ad"
        android:text="我确定！来吧！"
        android:textSize="21dp"
        android:layout_below="@+id/et_model"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/text2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="我也是无耻的广告君 =。=\n"
        android:layout_above="@+id/b_ad"
        android:textColor="#0c00ec"
        android:layout_centerHorizontal="true" />
    <Button
        android:layout_width="wrap_content"
        android:layout_height="196px"
        android:text="我也是广告 =.="
        android:textSize="39dp"
        android:background="@drawable/selector_ad"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:id="@+id/b_ad" />
    </RelativeLayout>
    <ImageButton
        android:layout_margin="7dp"
        android:layout_width="59dp"
        android:layout_height="45dp"
        android:id="@+id/ib_back"
        android:background="@drawable/abc_ic_ab_back_mtrl_am_alpha"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
    <WebView
        android:id="@+id/wv_load_brand"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone"/>
</RelativeLayout>
